{
  "name": "argentum-client",
  "version": "0.0.1",
  "browser": {
    "fs": false
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^3.0.1",
    "@types/pixi.js": "^4.7.1",
    "app": "^0.1.0",
    "assets": "^3.0.0",
    "babel-preset-env": "latest",
    "babel-preset-react": "latest",
    "enums": "^1.0.1",
    "jquery-ui": "^1.12.1",
    "jss": "latest",
    "parcel-bundler": "latest",
    "pixi.js": "^4.7.3",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-jss": "latest",
    "react-pixi-fiber": "^0.4.9",
    "storage": "^0.2.0",
    "styled-components": "^3.4.5",
    "ui": "^0.2.4",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-stage-2": "^6.24.1",
    "copyfiles": "^2.1.0",
    "eslint": "^4.13.1",
    "node-sass": "^4.9.3"
  },
  "scripts": {
    "start": "npm run copy && parcel client/index.html --out-dir dist ",
    "build": "parcel build client/index.html --public-url ./",
    "lint": "npx eslint --fix --ext jsx,js client",
    "copy": "npm run scss-compile && npm run copyGraphics && npm run copyAudio&& npm run copyIndex",
    "copyIndex": "copyfiles --flat --verbose --soft ./client/indices/*.json dist/indices",
    "copyGraphics": "copyfiles --flat --verbose --soft ./client/graficos/*.png dist/graficos",
    "copyAudio": "copyfiles --flat --verbose --soft ./client/audio/sonidos/*.webm dist/audio/sonidos",
    "scss-compile": "node-sass --include-path scss client/css/main.scss  client/css/main.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
