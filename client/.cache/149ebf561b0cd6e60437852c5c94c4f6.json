:"js/components/ChatBox.js","original":{"line":11,"column":6}},{"generated":{"line":117,"column":12},"source":"js/components/ChatBox.js","original":{"line":11,"column":12}},{"name":"props","generated":{"line":117,"column":18},"source":"js/components/ChatBox.js","original":{"line":11,"column":17}},{"generated":{"line":117,"column":23},"source":"js/components/ChatBox.js","original":{"line":11,"column":12}},{"name":"show","generated":{"line":117,"column":24},"source":"js/components/ChatBox.js","original":{"line":11,"column":23}},{"generated":{"line":118,"column":0},"source":"js/components/ChatBox.js","original":{"line":9,"column":17}},{"generated":{"line":118,"column":5},"source":"js/components/ChatBox.js","original":{"line":9,"column":4}},{"generated":{"line":119,"column":0},"source":"js/components/ChatBox.js","original":{"line":6,"column":21}},{"generated":{"line":120,"column":0},"source":"js/components/ChatBox.js","original":{"line":13,"column":3}},{"name":"React","generated":{"line":123,"column":2},"source":"js/components/ChatBox.js","original":{"line":5,"column":37}},{"name":"Component","generated":{"line":123,"column":18},"source":"js/components/ChatBox.js","original":{"line":5,"column":43}},{"name":"ChatBox","generated":{"line":125,"column":18},"source":"js/components/ChatBox.js","original":{"line":5,"column":21}}],"sources":{"js/components/ChatBox.js":"import React from 'react';\nimport TextField from '@material-ui/core/TextField';\nimport Button from '@material-ui/core/Button';\n\nexport default class ChatBox extends React.Component {\n  constructor(props) {\n    super(props);\n    this.props = props;\n    this.state = {\n      textBox: '',\n      show: this.props.show\n    }\n  }\n\n  handleChange = name => event => {\n    this.setState({\n      [name]: event.target.value,\n    });\n  };\n  submit = () => {\n    const {textBox} = this.state;\n    if (this.game.player) {\n      if (this.talkingToClan) {\n        this.game.client.sendGuildMessage(textBox);\n        this.talkingToClan = false;\n      } else {\n        let res = this.comandosChat.parsearChat(textBox);\n        if (res) {\n          this.game.client.sendTalk(res);\n        }\n      }\n    }\n  };\n  show = () => {\n    this.setState({show: true})\n  };\n  hide = () => {\n    this.setState({show: false})\n  };\n  render = () => {\n    const {textBox} = this.state;\n    return (\n      <div id=\"chatinputContainer\" className=\"inner_border_default\" onKeyPress={(ev) => {\n        if (this.game.isPaused || (this.game.gameUI.hayPopUpActivo())) {\n          return;\n        }\n        if (ev.key === 'Enter') {\n          // Do code here\n          ev.preventDefault();\n          this.show();\n        } else if (ev.key === 'Escape'){\n          ev.preventDefault();\n          this.hide();\n        }\n      }}>\n        <div style={{width: '100%', display: 'flex'}}>\n          <TextField\n            id=\"chatinput\"\n            defaultValue=\"\"\n            className=\"form-control\"\n            onChange={this.handleChange('textBox')}\n            maxLength=\"160\" autoComplete=\"off\"\n            value={textBox}\n            margin=\"normal\"\n            variant=\"outlined\"\n            onKeyPress={(ev) => {\n              if (ev.key === 'Enter') {\n                // Do code here\n                ev.preventDefault();\n                this.submit();\n              } else if (ev.key === 'Escape'){\n                ev.preventDefault();\n                this.hide();\n              }\n            }}\n          />\n          <Button id=\"botonChatInput\" className=\"botonImagen noClickSound\" onClick={this.submit}/>\n        </div>\n      </div>)\n  }\n}"},"lineCount":null}},"hash":"7b2928fd52417bb91d73b818cd41b381","cacheData":{"env":{}}}