},"source":"js/ui/popups/herreria.js","original":{"line":94,"column":9}},{"generated":{"line":150,"column":19},"source":"js/ui/popups/herreria.js","original":{"line":94,"column":4}},{"name":"items","generated":{"line":150,"column":20},"source":"js/ui/popups/herreria.js","original":{"line":94,"column":18}},{"generated":{"line":150,"column":25},"source":"js/ui/popups/herreria.js","original":{"line":94,"column":4}},{"generated":{"line":151,"column":0},"source":"js/ui/popups/herreria.js","original":{"line":95,"column":3}},{"name":"PopUp","generated":{"line":155,"column":2},"source":"js/ui/popups/herreria.js","original":{"line":8,"column":38}},{"name":"Herreria","generated":{"line":157,"column":18},"source":"js/ui/popups/herreria.js","original":{"line":8,"column":21}}],"sources":{"js/ui/popups/herreria.js":"/**\n * Created by horacio on 6/20/16.\n */\n\nimport DOMdata from '../../../menus/herreria';\nimport PopUp from './popup'; import React from 'react';\n\nexport default class Herreria extends PopUp {\n  constructor(game) {\n\n    let options = {\n      width: 500,\n      height: 400,\n      minWidth: 250,\n      minHeight: 300\n    };\n    super(DOMdata, options);\n\n    this.game = game;\n    //this.initCallbacks();\n    this.$itemsContainer = $('#herreriaContenedorItems');\n  }\n\n  /*Item contiene:\n       Name: Name,\n       GrhIndex: GrhIndex,\n       LingH: LingH,\n       LingP: LingP,\n       LingO: LingO,\n       ArmasHerreroIndex: ArmasHerreroIndex,\n       ObjUpgrade: ObjUpgrade,\n       */\n  setItems(items) {\n    //TODO objUpgrade\n\n    let self = this;\n    for (let item of items) {\n      let $row = $('<tr></tr>');\n\n      let numGraf = this.game.assetManager.getNumGraficoFromGrh(item.GrhIndex);\n      let url = 'url(../../graficos/' + numGraf + '.png)';\n\n      let $cell = $('<td></td>');\n      let $imagenItem = $('<div class=\"divImagen\" style=\"width: 50px; height:50px;\"></div>');\n      $imagenItem.css('background-image', url);\n      $cell.append($imagenItem);\n\n      $row.append($cell);\n\n      let $cellRequerimientos = $('<td></td>');\n      $cellRequerimientos.text('Require lingote hierro: ' + item.LingH + ' , lingote plata ' + item.LingP + ' y lingote de oro: ' + item.LingO);\n      // TODO: graficos madera y madera elfica\n      $row.append($cellRequerimientos);\n\n      let $cellConstruir = $('<td></td>');\n      let $botonConstruir = $('<button class=\"btn btn-default\" >Construir</button>');\n\n      $botonConstruir.data('itemIndex', item.ArmasHerreroIndex);\n      $botonConstruir.click(function () {\n        let cantidadAConstruir = $('#herreriaCantidadAConstruir').val();\n        self.game.client.sendInitCrafting(cantidadAConstruir, 1);//TODO: horrible esto, que se haga de 1 (cambiar sv)\n        let itemIndex = $(this).data('itemIndex');\n        self.game.client.sendCraftBlacksmith(itemIndex);\n      });\n      $cellConstruir.append($botonConstruir);\n      $row.append($cellConstruir);\n      this.$itemsContainer.append($row);\n    }\n  }\n\n  setWeapons(items) {\n    /*Item contiene:\n           Name: Name,\n           GrhIndex: GrhIndex,\n           LingH: LingH,\n           LingP: LingP,\n           LingO: LingO,\n           ArmasHerreroIndex: ArmasHerreroIndex,\n           ObjUpgrade: ObjUpgrade,\n           */\n    this.setItems(items);\n  }\n\n  setArmors(items) {\n    /* Item contiene\n           Name: Name,\n           GrhIndex: GrhIndex,\n           LingH: LingH,\n           LingP: LingP,\n           LingO: LingO,\n           ArmasHerreroIndex: ArmasHerreroIndex,\n           ObjUpgrade: ObjUpgrade,\n           */\n    this.setItems(items);\n  }\n}\n\n"},"lineCount":null}},"hash":"12895114b797d5fcef22c8be9090c9e4","cacheData":{"env":{}}}