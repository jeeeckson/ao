  } else {\n        Screenfull.exit();\n      }\n    });\n\n    $('#opcionesSliderPantalla').slider({\n      range: 'min',\n    });\n  }\n\n}\n"},"lineCount":null}},"hash":"25d7a76162fba84da38b35a61ae7fce8","cacheData":{"env":{}}}